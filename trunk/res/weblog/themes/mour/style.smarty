body {
text-align:center;
font-family: "Lucida Grande", Verdana, "Lucida Sans", sans, Hei;
color: #f0f0f0;
padding: 0;
background: #384658;
}
a, a:hover, a:visited {
color: #a3b1c3;
text-decoration: none;
}
a:hover {
color: #ccd4de;
}
#top {
margin-right: auto;
margin-left: auto;
text-align:left;
width: 700px;
}
#title {
font-weight: bolder;
border-bottom: 3px solid #495666;
font-size: 20px;
padding-bottom: 30px;
}
